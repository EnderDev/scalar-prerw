    "@types/react": "^17.0.18",
    "@types/react-currency-format": "^1.0.0",
    "@types/react-text-mask": "^5.4.9",
    "@types/remark-prism": "^1.3.0",
    "@types/showdown": "^1.9.4",
    "@types/tailwindcss": "^2.2.1",
    "autoprefixer": "^10.3.3",
    "glob": "^7.2.0",
    "gray-matter": "^4.0.3",
