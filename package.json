{
  "name": "my-razzle-app",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "start:tsc": "tsc -b -w --preserveWatchOutput",
    "start": "concurrently \"yarn start:tsc\" \"razzle start\"",
    "build": "tsc -b && razzle build",
    "start:prod": "NODE_ENV=production node build/server.js",
    "deploy": "yarn build && vercel --prod",
    "format": "prettier --write ."
  },
  "dependencies": {
    "@prisma/client": "^2.15.0",
    "@types/styled-components": "^5.1.7",
    "argon2": "^0.27.1",
    "axios": "^0.21.1",
    "bcrypt": "^5.0.0",
    "body-parser": "^1.19.0",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "razzle-plugin-typescript": "^3.3.11",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-router-dom": "^5.2.0",
    "styled-components": "^5.2.1",
    "tslint": "^6.1.3",
    "typescript": "^4.1.3"
  },
  "devDependencies": {
    "@prisma/cli": "^2.15.0",
    "@types/bcrypt": "^3.0.0",
    "@types/cors": "^2.8.9",
    "@types/express": "^4.17.11",
    "@types/node": "^14.14.22",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/react-router-dom": "^5.1.7",
    "concurrently": "^5.3.0",
    "prettier": "2.2.1",
    "razzle": "^3.3.11",
    "tslint-config-prettier": "^1.18.0"
  }
}
