{
  "name": "dothq.co",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "dev": "ruby scripts/environment-revision.rb && next dev",
    "build": "ruby scripts/environment-revision.rb && next build",
    "start": "next start",
    "lint": "next lint",
    "deploy:prod": "if [ $(git rev-parse --abbrev-ref HEAD) = \"main\" ]; then vercel --prod; else echo \"You need to be on the main branch to deploy to prod.\"; fi",
    "deploy:staging": "if [ $(git rev-parse --abbrev-ref HEAD) = \"staging\" ]; then vercel; else echo \"You need to be on the staging branch to deploy to staging.\"; fi",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "build:sitemap": "ts-node utils/generate-sitemap.ts",
    "gen:revision": "git rev-parse HEAD > ref",
    "preinstall": "ruby scripts/environment-revision.rb"
  },
  "dependencies": {
    "@dothq/id": "^1.0.1",
    "@giscus/react": "^1.0.0",
    "axios": "^0.21.1",
    "cheerio": "^1.0.0-rc.10",
    "chrome-aws-lambda": "^10.1.0",
    "countries-and-timezones": "^3.2.3",
    "document.contains": "^1.0.2",
    "dotenv": "^10.0.0",
    "flickity": "^2.2.2",
    "fs-extra": "^10.0.0",
    "markdown-to-text": "^0.1.1",
    "netlify-cms-app": "^2.15.48",
    "next": "^11.1.0",
    "next-intl": "^2.0.5",
    "next-useragent": "^2.6.0",
    "prismjs": "^1.25.0",
    "puppeteer-core": "^10.4.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-flickity-component": "^3.6.2",
    "react-number-format": "^4.7.3",
    "react-scrollmagic": "^2.3.0",
    "react-text-mask": "^5.4.3",
    "react-use-ripple": "^1.5.2",
    "react-use-scroll-position": "^2.0.0",
    "react-waypoint": "^10.1.0",
    "rehype": "^12.0.0",
    "rehype-autolink-headings": "^6.1.0",
    "rehype-highlight": "^5.0.0",
    "remark": "^14.0.1",
